{
  "name": "pcfg_tool",
  "version": "0.0.0",
  "author": "Guilherme Calabria Lopes",
  "bin": {
    "pcfg_tool": "./bin/run"
  },
  "bugs": "https://github.com/playground/pcfg_tool/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "n-readlines": "^1.0.1",
    "parsimmon": "^1.17.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@oclif/dev-cli": "^1",
    "@types/jest": "^26.0.23",
    "@types/n-readlines": "^1.0.1",
    "@types/node": "^10",
    "@types/parsimmon": "^1.10.6",
    "babel-jest": "^26.6.3",
    "eslint": "^7.0.0-rc.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^8",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/playground/pcfg_tool",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pcfg_tool",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "command_not_found": [
        "./lib/hooks/custom-command-not-found/custom-command-not-found"
      ]
    }
  },
  "repository": "playground/pcfg_tool",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
